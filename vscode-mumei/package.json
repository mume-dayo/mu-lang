{
  "name": "mumei-language",
  "displayName": "Mumei Language Support",
  "description": "Syntax highlighting and language support for Mumei (.mu) programming language",
  "version": "1.0.0",
  "publisher": "mumei-lang",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "mumei",
    "mu",
    "programming language",
    "syntax highlighting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mumei",
        "aliases": [
          "Mumei",
          "mumei"
        ],
        "extensions": [
          ".mu"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "mumei",
        "scopeName": "source.mumei",
        "path": "./syntaxes/mumei.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mumei-language"
  }
}
